    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.13.0",
    "browserify": "^13.0.1",
    "envify": "^3.4.1",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0-beta",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.5",
    "gulp-csso": "^2.0.0",
    "gulp-exit": "0.0.2",
