{
  "name": "openaq-status",
  "version": "0.0.1",
  "description": "Status page for OpenAQ.",
  "repository": {
    "type": "git",
    "url": "https://github.com/openaq/openaq-status.git"
  },
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
